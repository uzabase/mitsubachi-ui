{
  "name": "mitsubachi-ui",
  "version": "0.18.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./custom-elements.json": "./dist/custom-elements.json"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "rimraf dist && run-s build:*",
    "build:vite": "vite build",
    "build:custom-elements-manifest": "custom-elements-manifest analyze --config custom-elements-manifest.config.mjs",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "run-s \"format:* !format:prettier:check\"",
    "format:prettier": "prettier --write '(.storybook|src|stories|tests|tools)/**/*.ts'",
    "format:prettier:check": "prettier --check '(.storybook|src|stories|tests|tools)/**/*.ts'",
    "format:lint": "stylelint --fix 'src/**/*.css'",
    "test": "vitest",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzabase/mitsubachi-ui.git"
  },
  "files": [
    "dist"
  ],
  "license": "ISC",
  "dependencies": {
    "@sp-design/recet.css": "git+https://github.com/uzabase/sp-design-reset.css#semver:^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@storybook/addon-docs": "^9.1.2",
    "@storybook/web-components-vite": "^9.1.2",
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@vitest/browser": "^3.2.4",
    "dedent": "^1.6.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.2",
    "eslint-plugin-wc": "^3.0.1",
    "globals": "^16.3.0",
    "lit": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.54.2",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rimraf": "^6.0.1",
    "shadow-dom-testing-library": "^1.13.0",
    "storybook": "^9.1.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "yakuhanjp": "^4.1.1"
  },
  "customElements": "dist/custom-elements.json"
}
